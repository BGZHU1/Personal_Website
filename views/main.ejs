<script>
$(document).ready(function() {
  //console.log("welcome main page");
	var message = {

		message: [
      'Hi , My name is Bijie Zhu；',
      '你好，我的名字叫朱比洁；',
      'I study computer science at queens college;',
			'I am an aspired software developer;',
			'I like software development + machine learning ;',
			'I enjoy movie and music;',
      'I live with java, python, javascript, nodejs...;',
      '感谢你来我的小窝参观;',

		],
		counterS: 0,
		counterL: 0,
		deleteS: false,

		init: function() {
			this.cacheElem();
			this.type();
		},

		cacheElem: function() {
			this.$text = $('.text');
		},

		type: function() {
			var message 	= this.message[this.counterS],
				  that 	    = this,
				  speed 	  = 0;

			message = !this.deleteS ? message.slice(0, ++this.counterL) : message.slice(0, --this.counterL);
			if(this.message[this.counterS] != message && !this.deleteS) {
				this.$text.text(message);
				speed = 90;
			}
			else {
				this.deleteS = true;
				speed = this.message[this.counterS] == message ? 1000 : 40;
				this.$text.text(message);
				if (message == '') {
					this.deleteS = false;
					this.counterS = this.counterS < this.message.length - 1 ? this.counterS + 1 : 0;
				}
			}
			setTimeout(function(){that.type()}, speed);
		}
	};

	message.init();
});

</script>

  <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>




